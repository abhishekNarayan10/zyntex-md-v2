{
    "name": "@ybd-project/ytdl-core",
    "version": "5.0.12",
    "description": "YBD Project fork of ytdl-core.",
    "author": "YBD Project",
    "repository": {
        "type": "git",
        "url": "git://github.com/ybd-project/ytdl-core.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "engines": {
        "node": ">=18.0"
    },
    "main": "./package/YtdlCore.js",
    "types": "./package/YtdlCore.d.ts",
    "files": [
        "package"
    ],
    "scripts": {
        "build": "rmdir /s /q package && tsc && tsc-alias",
        "update": "ncu && ncu -u && npm i",
        "publish:github": "npm publish --registry=https://npm.pkg.github.com",
        "publish:npm": "npm publish --registry=https://registry.npmjs.org/",
        "publish:all": "npm run publish:github && npm run publish:npm"
    },
    "dependencies": {
        "http-cookie-agent": "^6.0.5",
        "m3u8stream": "^0.8.6",
        "miniget": "^4.2.3",
        "sax": "^1.4.1",
        "tough-cookie": "^4.1.4",
        "undici": "five",
        "youtube-po-token-generator": "^0.2.0"
    },
    "devDependencies": {
        "@types/node": "^22.4.2",
        "@types/sax": "^1.2.7",
        "eslint": "^9.9.0",
        "npm-check-updates": "^17.0.6",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.5.4"
    },
    "keywords": [
        "youtube",
        "video",
        "download",
        "ybd-project",
        "ytdl"
    ],
    "license": "MIT"
}
